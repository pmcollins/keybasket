<%= breadcrumbs([@company, @property, @unit, @rental_agreement, @fee]) %>

<fieldset>
  <legend>Fee</legend>
  <p>
    <span class="name">Created at:</span>
    <span class="value"><%= @fee.created_on %></span>
  </p>
  <p>
    <span class="name">Amount:</span>
    <span class="value"><%= number_to_currency(@fee.amount) %></span>
  </p>
  <p>
    <span class="name">Notes:</span>
    <span class="value"><%= @fee.notes %></span>
  </p>
</fieldset>
