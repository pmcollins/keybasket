<label>Name</label><br/>
<%= form.text_field :name %><br/>
<% fields_for :address do |a| %>
<p>
  <label>Address Line 1</label><br/>
  <%= a.text_field :line1 %>
</p>
<p>
  <label>Address Line 2</label><br/>
  <%= a.text_field :line2 %><br/>
</p>
<p>
  <label>City</label><br/>
  <%= a.text_field :city %><br/>
</p>
<p>
  <label>State</label><br/>
  <%= a.text_field :state %><br/>
</p>
<p>
  <label>Zip</label><br/>
  <%= a.text_field :zip %>
</p>
<% end %>
